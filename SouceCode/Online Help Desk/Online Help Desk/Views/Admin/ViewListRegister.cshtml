@model IEnumerable<Online_Help_Desk.Models.Register>

@{
    ViewBag.Title = "ViewListRegister";
    Layout = "~/Views/Shared/_LayoutIndexAdmin.cshtml";
}

<div class="col-sm-3 sidenav">
    <h4>Perfect Technological Innovation</h4>
    <ul class="nav nav-pills nav-stacked">
        <li>@Html.ActionLink("Index", "Index", "Admin")</li>
        <li>@Html.ActionLink("View List Staff", "DetailListStaff", "Admin")</li>
        <li>@Html.ActionLink("View List End-User", "DetailListUser", "Admin")</li>
        <li  class="active">@Html.ActionLink("View List Register", "ViewListRegister", "Admin")</li>
        <li><a href="#section3">Change Password</a></li>
    </ul>

</div>

<div class="col-sm-9">
    <table class="table">
        <h2>ViewListRegister</h2>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FullName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Gender)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Phone)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Address)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DOB)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.FullName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Gender)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Phone)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Address)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Email)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.DOB)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.RegisterID }) |
                    @Html.ActionLink("Details", "Details", new { id = item.RegisterID }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.RegisterID })
                </td>
            </tr>
        }

    </table>
</div>