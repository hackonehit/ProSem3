@model IEnumerable<Online_Help_Desk.Models.User>

@{
    ViewBag.Title =  "List End-user";
    Layout = "~/Views/Shared/_LayoutIndexAdmin.cshtml";
}
<div class="col-sm-3 sidenav">
    <h4>Perfect Technological Innovation</h4>
    <ul class="nav nav-pills nav-stacked">
        <li>@Html.ActionLink("Index", "Index", "Admin")</li>
        <li>@Html.ActionLink("View List Staff", "DetailListStaff", "Admin")</li>
        <li class="active">@Html.ActionLink("View List End-User", "DetailListUser", "Admin")</li>
        <li>@Html.ActionLink("View List Register", "ViewListRegister", "Admin")</li>
        <li><a href="#section3">Change Password</a></li>
    </ul>

</div>
<div class="col-sm-9">
    <h2>Detai lList End-User</h2>
    @Html.ActionLink("Create New", "CreateEndUser", "Admin")
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.EUUsername)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DOB)
            </th>

            <th>@Html.DisplayNameFor(model => model.Address)</th>
            <th>@Html.DisplayNameFor(model => model.Gender)</th>
            <th>@Html.DisplayNameFor(model => model.Phone)</th>
            <th>
                @Html.DisplayNameFor(model => model.EUStatus)
            </th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.EUPassword)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Email)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.DOB)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Address)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Gender)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Phone)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.EUStatus)
                </td>
                <td>
                    @Html.ActionLink("Edit", "EditInforUser", new { id = item.EUUsername }) |
                    @Html.ActionLink("Details", "DetailsInfoUser", new { id = item.EUUsername }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.EUUsername })
                </td>
            </tr>
        }

    </table>
</div>
