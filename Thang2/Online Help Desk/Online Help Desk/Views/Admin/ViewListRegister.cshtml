@model IEnumerable<Online_Help_Desk.Models.Register>

<table class="table">
    <tr>
        <th>
            FullName
        </th>
        <th>
            Gender
        </th>
        
        
        <th>
            Email
        </th>
        
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FullName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Gender)
        </td>
        
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        
        <td>
            
            @Html.ActionLink("Details", "DetailsRegister", new { id = item.RegisterID })
        </td>
    </tr>
}

</table>
@Html.ActionLink("Back","Index","Admin")
